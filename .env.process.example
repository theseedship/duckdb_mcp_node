# Process Mining Parquet URLs Configuration
# Copy this file to .env and configure your parquet URLs

# Process summary parquet (high-level metadata)
# Supports {doc_uuid} or {doc_id} placeholders for per-document files
# Example: s3://bucket/parquet/{doc_uuid}/process_summary.parquet
PROCESS_SUMMARY_URL=

# Process steps parquet (individual workflow steps with embeddings)
# Example: s3://bucket/parquet/{doc_uuid}/process_steps.parquet
PROCESS_STEPS_URL=

# Process edges parquet (connections between steps)
# Example: s3://bucket/parquet/{doc_uuid}/process_edges.parquet
PROCESS_EDGES_URL=

# Process signature parquet (global process embeddings for similarity search)
# Example: s3://bucket/parquet/{doc_uuid}/process_signature.parquet
PROCESS_SIGNATURE_URL=

# Optional: Temporal event data
PROCESS_EVENTS_URL=

# Optional: Temporal interval data
PROCESS_INTERVALS_URL=

# Optional: Time-series metrics
PROCESS_METRIC_TIME_URL=

# Optional: State deadlines
PROCESS_STATE_DEADLINE_URL=

# Optional: Embedding service for process.similar queries
# If not configured, you must provide pre-computed embeddings
EMBEDDING_SERVICE_URL=http://localhost:8000/embed
EMBEDDING_MODEL=all-MiniLM-L6-v2

# DuckDB configuration (inherited from main .env)
# DUCKDB_MEMORY=4GB
# DUCKDB_THREADS=4
# MCP_SECURITY_MODE=development
